[core]
  editor = vim
	excludesfile = ~/.gitignore
[user]
	name = Joshua Murray
  email = joshua.murray@gmail.com
[color]
  status = auto
  branch = auto
  interactive = auto
  diff = auto
[github]
  user = joshcom
[merge]
    tool = vimdiff
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = green
    changed = red
    untracked = cyan
[credential]
	helper = cache --timeout=3600

